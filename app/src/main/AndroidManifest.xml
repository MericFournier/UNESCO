<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.mericfournier.unesco">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".LoginApplication"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.TEST" />
            </intent-filter>
        </activity>
        <activity android:name=".homeActivity">
            <intent-filter>
                <action android:name="android.intent.action.HOME" />
            </intent-filter>
        </activity>
        <activity android:name=".LieuActivity">
            <intent-filter>
                <action android:name="android.intent.action.LIEU" />
            </intent-filter>
        </activity>
        <activity android:name=".CritereActivity">
            <intent-filter>
                <action android:name="android.intent.action.CRITERE" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity android:name=".FavorisActivity">
            <intent-filter>
                <action android:name="android.intent.action.BOOK" />
            </intent-filter>
        </activity>
        <activity android:name=".ItemLieuActivity"></activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity" />
    </application>

</manifest>